#navbar-collapse-1.collapse.navbar-collapse
  %ul.nav.navbar-nav.navbar-right
    / TODO: Check for signed-in user
    / - if user_signed_in?
    - if true
      %li
        = link_to customers_path, method: :delete do
          %span.glyphicon.glyphicon-off
          Logout
    - else
      %li{class: (params[:controller] == "devise/sessions") ? "active" : nil}
        = link_to customers_path do
          %span.glyphicon.glyphicon-off
          Login